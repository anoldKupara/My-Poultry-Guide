<div class="container-xxl flex-grow-1 container-p-y">
  <p id="notice"><%= notice %></p>

  <p>
    <strong>Name:</strong>
    <%= @expense.name %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= @expense.quantity %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= @expense.price %>
  </p>

  <p>
    <strong>Currency:</strong>
    <%= @expense.currency.name %>
  </p>

  <%= link_to 'Edit', edit_expense_path(@expense) %> |
  <%= link_to 'Back', expenses_path %>
</div>